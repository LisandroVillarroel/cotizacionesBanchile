{
  "name": "cotizaciones-banchile",
  "version": "20.0.0",
  "scripts": {
    "ng": "ng",
    "start:dev": "ng serve --configuration=dev",
    "start:prod": "ng serve --configuration=production",
    "build:dev": "ng build --configuration=dev",
    "build:prod": "ng build --configuration=production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:edge:local": "ng test --no-watch --browsers=EdgeHeadless --progress=false",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "format": "prettier --write \"src/**/*.{ts,html,css,scss}\"",
    "format:check": "prettier --check \"src/**/*.{ts,html,css,scss}\"",
    "ci:install": "npm ci",
    "ci:build": "npm run build:prod",
    "build:meta": "ng build --configuration production --stats-json",
    "analyze": "esbuild-visualizer --metadata ./dist/cotizaciones-banchile/stats.json --template treemap --filename stats.html",
    "analyze:bundle": "npm run build:meta && npm run analyze",
    "analyze:open": "npm run analyze && (xdg-open stats.html || open stats.html || start stats.html)",
    "depcruise:init": "depcruise --init",
    "depcruise:html": "depcruise \"src\" --include-only \"^src\" --output-type html > dependency-report.html",
    "depcruise:svg": "depcruise \"src\" --include-only \"^src\" --output-type dot | dot -T svg > dependency-graph.svg",
    "depcruise:check": "depcruise \"src\" --include-only \"^src\" --config .dependency-cruiser.js --output-type err",
    "depcruise:report": "npm run depcruise:html && (xdg-open dependency-report.html || open dependency-report.html || start dependency-report.html)"
  },
  "private": true,
  "engines": {
    "node": ">=18.13.0 <19.0.0",
    "npm": ">=8.19.0"
  },
  "dependencies": {
    "@angular/animations": "^17.3.0",
    "@angular/cdk": "^17.3.10",
    "@angular/common": "^17.3.0",
    "@angular/compiler": "^17.3.0",
    "@angular/core": "^17.3.0",
    "@angular/forms": "^17.3.0",
    "@angular/material": "^17.3.10",
    "@angular/material-moment-adapter": "^17.3.10",
    "@angular/platform-browser": "^17.3.0",
    "@angular/platform-browser-dynamic": "^17.3.0",
    "@angular/router": "^17.3.0",
    "@fdograph/rut-utilities": "^1.4.0",
    "@primeng/themes": "^20.1.1",
    "bootstrap": "^5.3.7",
    "chart.js": "^4.5.0",
    "crypto-js": "^4.2.0",
    "moment": "^2.30.1",
    "ngx-mask": "^17.1.8",
    "primeflex": "^4.0.0",
    "primeicons": "^7.0.0",
    "primeng": "^17.18.15",
    "rxjs": "~7.8.0",
    "sweetalert2": "^11.25.0",
    "tailwindcss": "^3.4.18",
    "tslib": "^2.3.0",
    "typescript": "5.3.x",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.17",
    "@angular-devkit/schematics": "^17.3.17",
    "@angular-eslint/builder": "17.0.1",
    "@angular-eslint/eslint-plugin": "17.0.1",
    "@angular-eslint/eslint-plugin-template": "17.0.1",
    "@angular-eslint/schematics": "^17.5.3",
    "@angular-eslint/template-parser": "17.0.1",
    "@angular/cli": "^17.3.17",
    "@angular/compiler-cli": "^17.3.0",
    "@eslint/js": "^9.39.1",
    "@types/crypto-js": "^4.2.2",
    "@types/eslint__js": "^8.42.3",
    "@types/jasmine": "~5.1.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^18.18.0",
    "@typescript-eslint/eslint-plugin": "6.13.2",
    "@typescript-eslint/parser": "6.13.2",
    "dependency-cruiser": "^16.10.4",
    "esbuild-visualizer": "^0.7.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-boundaries": "^4.2.0",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.5.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-coverage": "~2.2.0",
    "karma-edgium-launcher": "^4.0.0-0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prettier": "^3.2.0",
    "typescript-eslint": "^8.48.1"
  }
}
